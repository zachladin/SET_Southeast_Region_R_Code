	urldate = {2015-02-04},
	date = {2015},
	langid = {english},
	keywords = {América Central, Central America, conectividad migratoria, deforestación tropical, geolocalizador, geolocator, Hylocichla mustelina, migration network, migration route, migratory connectivity, red migratoria, ruta de migración, tropical deforestation},
	file = {Full Text PDF:/Users/zach/Library/Application Support/Zotero/Profiles/52mvp218.default/zotero/storage/DF9R69V5/Stanley et al. - 2015 - Connectivity of wood thrush breeding, wintering, a.pdf:application/pdf;Snapshot:/Users/zach/Library/Application Support/Zotero/Profiles/52mvp218.default/zotero/storage/FB537CZK/abstract.html:text/html}
