@article{kato_prey_2004,
	title = {Prey use by web-building spiders: stable isotope analyses of trophic flow at a forest-stream ecotone},
	volume = {19},
	doi = {10.1111/j.1440-1703.2004.00678.x},
	shorttitle = {Prey use by web-building spiders},
	abstract = {A forest-stream trophic link was examined by stable carbon isotope analyses which evaluated the relationship of aquatic insects emerging from a stream to the diets of web-building spiders. Spiders, aquatic and terrestrial prey, and basal resources of forest and stream food webs were collected in a deciduous forest along a Japanese headwater stream during May and July 2001. The delta(13)C analyses suggested that riparian tetragnathid spiders relied on aquatic insects and that the monthly variation of such dependence is partly associated with the seasonal dynamics of aquatic insect abundance in the riparian forest. Similarly, linyphiid spiders in the riparian forest exhibited delta(13)C values similar to aquatic prey in May. However, their delta(13)C values were close to terrestrial prey in both riparian and upland (150 m away from the stream) forests during June to July, suggesting the seasonal incorporation of stream-derived carbon into their tissue. In contrast, araneid spiders relied on terrestrial prey in both riparian and upland forests throughout the study period. These isotopic results were consistent with a previous study that reported seasonal variation in the aquatic prey contribution to total web contents for each spider group in this forest, implying that spiders assimilate trapped prey and that aquatic insect flux indeed contributes to the energetics of riparian tetragnathid and linyphiid spiders.},
	pages = {633--643},
	number = {6},
	journaltitle = {Ecological Research},
	author = {Kato, C. and Iwata, T. and Wada, E.},
	date = {2004-11},
	note = {{WOS}:000225061300008}
}

