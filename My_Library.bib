@article{morrissey_diet_2010,
	title = {Diet shifts during egg laying: Implications for measuring contaminants in bird eggs},
	volume = {158},
	doi = {10.1016/j.envpol.2009.08.040},
	shorttitle = {Diet shifts during egg laying},
	abstract = {We combined stable isotope tracers of blood plasma, blood cells and egg contents with faecal analysis during pre-breeding and egg laying phases in two dipper species Cinclus cinclus and Cinclus mexicanus to determine the occurrence of dietary shifts during egg production and to assess consequences for egg contaminant loads. In both species, changes in delta(13)C (C cinclus) or delta(15)N (C mexicanus) in female plasma relative to red blood cells indicated a dietary shift during laying that was not observed in males. Eurasian dippers increased prey consumption as breeding approached, shifting from primarily trichopteran insect larvae to ephemeropterans and plecopterans. In American dippers, egg-laying females switched to feeding at a higher trophic level by consuming more fish. Eggs derived from higher trophic level diets contained more mercury (American dipper), polychlorinated biphenyls and some organochlorines, especially {DDT} metabolites. The results demonstrate how dietary changes during egg laying accompany the demands for egg production with consequences for contaminant deposition in avian eggs. (C) 2009 Elsevier Ltd. All rights reserved.},
	pages = {447--454},
	number = {2},
	journaltitle = {Environmental Pollution},
	author = {Morrissey, Christy A. and Elliott, John E. and Ormerod, Stephen J.},
	date = {2010-02},
	note = {{WOS}:000274321600014}
}

